<div class="container">

      <h1>All articles</h1>
      <ul>
        <% @articles.each do |article| %>
        <li><%=  link_to article.title, article_path(article) %></li>
        <% end %>
      </ul>

    <button class="btn btn-warning" value='Add a new article'>
      <%= link_to 'Add new article', articles_new_path%>
    </button>

</div
